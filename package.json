{
  "name": "olc-auto",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "dev-server": "npx nodemon -w ./src/backpart/web-services.js -w ./src/client/components/js/utils.js",
    "build": "vite build",
    "preview": "vite preview",
    "ws-transpile": "babel ./src/server/web-services.js ./src/client/components/js/utils.js ./src/client/components/js/common.js --out-dir dist-server",
    "ws-clean": "rimraf dist-server",
    "ws-start": "node ./dist-server/web-services.js",
    "ws-build": "npm-run-all ws-clean ws-transpile",
    "ws-dev": "set NODE_ENV=development && npm-run-all ws-build ws-start",
    "ws-prod": "set NODE_ENV=production && npm-run-all ws-build ws-start",
    "start": "npm run ws-prod",
    "watch:dev": "npx nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run ws-dev",
    "watch": [
      "./src/web-services.js",
      "./src/components/js/utils.js"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "cors": "^2.8.5",
    "rimraf": "^3.0.2",
    "vue": "^3.2.25",
    "vue-web-cam": "^1.9.0"
  },
  "devDependencies": {
    "@babel/node": "^7.17.10",
    "@types/splitpanes": "^2.2.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "axios": "^0.27.2",
    "babel-preset-vite": "^1.0.4",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.14",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sass": "^1.51.0",
    "splitpanes": "^3.1.1",
    "vite": "^2.9.8",
    "vite-plugin-require-transform": "^1.0.3",
    "vue-router": "^4.0.14",
    "webrtc-adapter": "^8.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "babel-preset-vite"
    ]
  }
}
