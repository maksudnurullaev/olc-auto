{
  "name": "olc-auto",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev-front": "vite",
    "dev-server": "npx nodemon -w ./src/server -w ./src/utils",
    "build-front": "vite build",
    "preview": "vite preview",
    "ws-transpile-server": "babel ./src/server --out-dir dist/server",
    "ws-transpile-common": "babel ./src/utils --out-dir dist/utils",
    "ws-clean": "rimraf dist/server dist/utils",
    "ws-start": "node ./dist/server/apps.js",
    "ws-build": "npm-run-all build-front ws-clean ws-transpile-server ws-transpile-common",
    "ws-dev": "set NODE_ENV=development && npm-run-all ws-build ws-start",
    "ws-prod": "set NODE_ENV=production && npm-run-all ws-build ws-start",
    "start": "npm run ws-prod",
    "watch:dev": "npx nodemon",
    "test": "jest"
  },
  "jest": {
    "modulePathIgnorePatterns": ["dist"]  
  },
  "nodemonConfig": {
    "exec": "npm run ws-dev",
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "better-sqlite3": "^7.5.3",
    "cors": "^2.8.5",
    "knex": "^2.0.0",
    "objection": "^3.0.1",
    "rimraf": "^3.0.2",
    "vue": "^3.2.25",
    "vue-web-cam": "^1.9.0"
  },
  "devDependencies": {
    "@babel/node": "^7.17.10",
    "@types/splitpanes": "^2.2.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "axios": "^0.27.2",
    "babel-preset-vite": "^1.0.4",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jest": "^28.1.0",
    "jest-extended": "^2.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.14",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sass": "^1.51.0",
    "splitpanes": "^3.1.1",
    "superagent": "^7.1.3",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-require-transform": "^1.0.3",
    "vue-router": "^4.0.14",
    "webrtc-adapter": "^8.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "babel-preset-vite"
    ]
  }
}
